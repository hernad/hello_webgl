{
  "name": "RNAL 3D",
  "version": "1.0",
  "manifest_version": 2,
  "description": "RNAL 3d",

  "default_locale": "en",
  
  "offline_enabled": true,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },

  "homepage_url": "http://www.bring.out.ba",
  
  "options_page" : "options.html",

  "icons": { "16": "kobo_Knife.png",
             "48": "kobo_Knife.png",
            "128": "kobo_Knife.png" },
			  
  "permissions": [
    "http://api.flickr.com/",
    "http://api.test.com/"
  ],
  
  "requirements": {
    "3D": {
      "features": ["css3d", "webgl"]
    }
  }
  
}
